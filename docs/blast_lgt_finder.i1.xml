<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
        <name>btab2taxon compute</name>
        <command>
            <type>RunUnixCommand</type>
            <name>Filter BLAST for best hits</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/filter_lgt_best_hit</executable>
            <param>  
                <key>--input</key>
                <value>$;I_FILE_PATH$;</value>
            </param>
            <param>  
                <key>--trace_mapping</key>
                <value>$;MAPPING_FILE$;</value>
            </param>
            <param>  
                <key>--lineage1</key>
                <value>$;LINEAGE1$;</value>
            </param>
            <param>  
                <key>--lineage2</key>
                <value>$;LINEAGE2$;</value>
            </param>
            <param>  
                <key>--filter_lineage</key>
                <value>$;FILTER_LINEAGE$;</value>
            </param>
            <param>  
                <key>--filter_min_overlap</key>
                <value>$;FILTER_MIN_OVERLAP$;</value>
            </param>
            <param>
                <key>--ncbitax</key>
                <value>$;NCBITAX$;</value>
            </param>
            <param>  
                <key>--gitax</key>
                <value>$;GI2TAX$;</value>
            </param>
            <param>  
                <key>--dbhost</key>
                <value>$;HOST$;</value>
            </param>
            <param>  
                <key>--taxondb</key>
                <value>$;TAXONDB$;</value>
            </param>
            <param>  
                <key>--taxoncoll</key>
                <value>$;TAXONCOLL$;</value>
            </param>            
            <param>  
                <key>--overwrite</key>
                <value>$;OVERWRITE$;</value>
            </param> 
            <param>  
                <key>--idx_dir</key>
                <value>$;IDX_DIRECTORY$;</value>
            </param> 
            <param>  
                <key>--output_dir</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/</value>
            </param>             
            <param>  
                <key>stdout</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.filter.stdout</value>
            </param>
            <param>
                <key>stderr</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.filter.stderr</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>Create list of filtered files</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/create_list_file</executable>
            <param>  
                <key>--directory</key>
                <value>$;OUTPUT_DIRECTORY$;</value>
            </param>
            <param>  
                <key>--regex</key>
                <value>".*\.out"</value>
            </param>
            <param>  
                <key>--output_list</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/filter_btab.list</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>Find LGT</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/lgt_finder</executable>
            <param>  
                <key>--input_file_list</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/filter_btab.list</value>
            </param>
            <param>  
                <key>--output_prefix</key>
                <value>$;I_FILE_BASE$;</value>
            </param>
            <param>  
                <key>--append_links</key>
                <value>$;APPEND_LINKS$;</value>
            </param>
            <param>  
                <key>--output_dir</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/</value>
            </param>       
            <param>  
                <key>stdout</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.lgt_finder.stdout</value>
            </param>
            <param>
                <key>stderr</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.lgt_finder.stderr</value>
            </param>     
        </command>
    </commandSet>
</commandSetRoot>
