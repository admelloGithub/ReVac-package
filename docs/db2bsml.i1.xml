<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"			xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
	<name>db2bsml</name>
	<command>
	    <type>RunUnixCommand</type>
	    <state>incomplete</state>
	    <name>Create the FASTA output directory</name>
	    <executable>mkdir</executable>
	    <arg>-p -m 777 $;FASTA_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/</arg>
	</command>
	<command>
	    <type>RunUnixCommand</type>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/db2bsml</executable>
	    <name>db2bsml</name>
	    <param>
		<key>--database</key>
		<value>$;DATABASE$;</value>
	    </param>
	    <param>
		<key>--password</key>
		<value>$;PASSWORD$;</value>
	    </param>
	    <param>
		<key>--username</key>
		<value>$;USERNAME$;</value>
	    </param>
            <param>
		<key>--server</key>
		<value>$;HOST$;</value>
	    </param>
            <param>
		<key>--vendor</key>
		<value>$;RDBMS$;</value>
	    </param>
	    <param>
		<key>--outfile</key>
		<value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;DATABASE$;_$;ASMBL_ID$;_assembly.bsml</value>
	    </param>
	    <param>
		<key>--logfile</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;DATABASE$;.$;ASMBL_ID$;.db2bsml.pl.log</value>
	    </param>
	    <param>
		<key>--debug_level</key>
		<value>$;DEBUG$;</value>
	    </param>
	    <param>
		<key>--fastadir</key>
		<value>$;FASTA_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/</value>
	    </param>
	    <param>
		<key>--asmbl_id</key>
		<value>$;ASMBL_ID$;</value>
	    </param>
	    <param>
		<key>--id_repository</key>
		<value>$;PROJECT_ID_REPOSITORY$;</value>
	    </param>
	    <param>
		<key>--in-id-map-dir</key>
		<value>'$;INPUT_ID_MAPPING_DIRECTORIES$;'</value>
	    </param>
	    <param>
		<key>--in-id-map-file</key>
		<value>'$;INPUT_ID_MAPPING_FILES$;'</value>
	    </param>
	    <param>
		<key>--out_id_map_file</key>
		<value>$;TMP_DIR$;/id_mapping/$;DATABASE$;_$;ASMBL_ID$;.idmap</value>
	    </param>
	    <param>
		<key>--sourcename</key>
		<value>$;OUTPUT_DIRECTORY$;</value>
	    </param>
	    <param>
		<key>stdout</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/db2bsml.$;DATABASE$;.$;ASMBL_ID$;.stdout</value>
	    </param>
	    <param>
		<key>stderr</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/db2bsml.$;DATABASE$;.$;ASMBL_ID$;.stderr</value>
	    </param>
	</command>
	<command>
	    <type>RunUnixCommand</type>
	    <state>incomplete</state>
	    <name>DTD validation</name>
	    <executable>$;BIN_DIR$;/dtdValid.pl</executable>
	    <arg>-d $;BSML_DTD$; $;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;DATABASE$;_$;ASMBL_ID$;_assembly.bsml</arg>
	</command>
	<command>
	    <type>RunUnixCommand</type>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/profileBSMLFile</executable>
	    <name>profileBSMLFile</name>
	    <param>
		<key>--infile</key>
		<value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;DATABASE$;_$;ASMBL_ID$;_assembly.bsml</value>
	    </param>
	    <param>
		<key>--database</key>
		<value>$;DATABASE$;</value>
	    </param>
	    <param>
		<key>--asmbl_id</key>
		<value>$;ASMBL_ID$;</value>
	    </param>
	    <param>
		<key>--logfile</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;DATABASE$;.$;ASMBL_ID$;.profileBSMLFile.pl.log</value>
	    </param>
	    <param>
		<key>stdout</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/profileBSMLFile.$;DATABASE$;.$;ASMBL_ID$;.stdout</value>
	    </param>
	    <param>
		<key>stderr</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/profileBSMLFile.$;DATABASE$;.$;ASMBL_ID$;.stderr</value>
	    </param>
	</command>
    </commandSet>
</commandSetRoot>
