<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
	<name>Load BCP files</name>
	<command>
	    <type>RunUnixCommand</type>
	    <state>incomplete</state>
	    <name>Load BCP file into database table</name>
	    <executable>$;BIN_DIR$;/flatFileToChado</executable>
	    <param>
		<key>--username</key>
		<value>chado_admin</value>
	    </param>
	    <param>
		<key>--password</key>
		<value>chado_admin99</value>
	    </param>
	    <param>
		<key>--database</key>
		<value>$;DATABASE$;</value>
	    </param>
	    <param>	  
		<key>--database_type</key>
		<value>$;DATABASE_TYPE$;</value>
	    </param>
	    <param>
		<key>--bcp_ext</key>
		<value>$;BCP_EXTENSION$;</value>
	    </param>
	    <param>
		<key>--server</key>
		<value>$;SERVER$;</value>
	    </param>
	    <param>
		<key>--debug_level</key>
		<value>$;DEBUG$;</value>
	    </param>
	    <param>
		<key>--bcpmode</key>
		<value>in</value>
	    </param>
	    <param>
		<key>--batchsize</key>
		<value>$;BATCHSIZE$;</value>
	    </param>
	    <param>
		<key>--logfile</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;/flatFileToChado.pl.log</value>
	    </param>
	    <param>
		<key>--infile</key>
		<value>$;I_FILE_BASE$;</value>
	    </param>
	    <param>
		<key>stdout</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;/flatFileToChado.pl.stdout</value>
	    </param>
	    <param>
		<key>stderr</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;/flatFileToChado.pl.stderr</value>
	    </param>
        </command>
    </commandSet>
</commandSetRoot>
