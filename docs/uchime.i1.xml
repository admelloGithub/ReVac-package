<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
        <name>chimera chimera detection compute</name>
        <command>
            <type>RunUnixCommand</type>
            <name>uchime chimera checker</name>
            <state>incomplete</state>
            <executable>$;UCHIME_EXEC$;</executable>
            <arg> --input $;I_FILE_PATH$; --db $;TEMPLATE_FILE$; --uchimeout $;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.uchime --minh $;MINH$; --mindiv $;MINDIVERGENCE$;</arg>
            <param>
                <key>stderr</key>
                <value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.stderr</value>
            </param>
            <param>
                <key>stdout</key>
                <value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.stdout</value>
            </param>
        </command>
    </commandSet>
</commandSetRoot>

