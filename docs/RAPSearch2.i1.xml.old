<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
        <name>RAPSearch2 compute</name>
        <command>
            <type>RunUnixCommand</type>
            <name>RAPSearch2</name>
            <state>incomplete</state>
            <executable>$;RAPSEARCH_EXEC$;</executable>
			<arg> -q $;I_FILE_PATH$; -d $;DATABASE_PATH$; -o $;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$; -s F -e $;EXPECT$; -l $;MIN_ALN_LENGTH$; -b $;DATABASE_MATCHES$; -v $;DESCRIPTIONS$; $;OTHER_OPTS$;</arg>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>convert m8 to btab</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/RAPSearch2btab</executable>
            <param>  
                <key>--input_file</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.m8</value>
            </param>
            <param>  
                <key>--output_file</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.btab</value>
			</param>
			<param>
				<key>--uniprot_db</key>
				<value>$;BTAB_SQLITE_DB$;</value>
			</param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>convert btab to bsml</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/blastbtab2bsml</executable>
            <param>
                <key>--query_file_path</key>
                <value>$;I_FILE_PATH$;</value>
            </param>
            <param>
                <key>--btab_file</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.btab</value>
            </param>
            <param>
                <key>--output</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml</value>
            </param>
            <param>
                <key>--pvalue</key>
                <value>$;EXPECT$;</value>
            </param>
            <param>
                <key>--class</key>
                <value>polypeptide</value>
            </param>
            <param>
                <key>--analysis_id</key>
                <value>$;COMPONENT_NAME$;_analysis</value>
            </param>
            <param>
                <key>--gzip</key>
                <value>$;COMPRESS_BSML_OUTPUT$;</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>store config params</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/analysis2bsml</executable>
            <param>  
                <key>--ergatis_control_file</key>
                <value>$;ERGATIS_DIR$;.ini</value>
            </param>
            <param>  
                <key>--conf</key>
                <value>$;COMPONENT_CONFIG$;</value>
            </param>
            <param>  
                <key>--bsml_file</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml</value>
            </param>
            <param>  
                <key>--componenturl</key>
                <value>$;WORKFLOW_REPOSITORY$;/component.xml</value>
            </param>
            <param>  
                <key>--pipelineurl</key>
                <value>$;REPOSITORY_ROOT$;/workflow/runtime/pipeline/$;PIPELINEID$;/pipeline.xml</value>
            </param>
            <param>  
                <key>--analysis_id</key>
                <value>$;COMPONENT_NAME$;_analysis</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>compress m8 output</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/compress_file</executable>
            <param>  
                <key>--file</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.m8</value>
            </param>
            <param>  
                <key>--compress</key>
                <value>$;COMPRESS_RAW_OUTPUT$;</value>
            </param>
            <param>  
                <key>--list_file</key>
                <value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.m8.list</value>
            </param>
		</command>
        <command>
            <type>RunUnixCommand</type>
            <name>compress aln output</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/compress_file</executable>
            <param>  
                <key>--file</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.aln</value>
            </param>
            <param>  
                <key>--compress</key>
                <value>$;COMPRESS_RAW_OUTPUT$;</value>
            </param>
            <param>  
                <key>--list_file</key>
                <value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.aln.list</value>
            </param>
		</command>
        <command>
            <type>RunUnixCommand</type>
            <name>dtd validation</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/dtdValid.pl</executable>
            <param>  
                <key>-d</key>
                <value>$;DOCS_DIR$;/bsml3_1.dtd</value>
            </param>
            <arg>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml</arg>
        </command>
    </commandSet>
</commandSetRoot>
