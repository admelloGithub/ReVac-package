<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"			xsi:schemaLocation='commandSet.xsd'>
  <commandSet type="serial">
    <name>load bcp files</name>
      <command>
	  <type>RunUnixCommand</type>
	  <name>load tables</name>
	  <state>incomplete</state>
	  <executable>$;BIN_DIR$;/flatFileToChado</executable>
	  <param>
	      <key>--username</key>
	      <value>$;USERNAME$;</value>
	  </param>
	  <param>
	      <key>--password</key>
	      <value>'$;PASSWORD$;'</value>
	  </param>
	  <param>
	      <key>--database</key>
	      <value>$;DATABASE$;</value>
	  </param>
	  <param>
	      <key>--server</key>
	      <value>$;SERVER$;</value>
	  </param>
	  <param>	  
	      <key>--database_type</key>
	      <value>$;RDBMS$;</value>
	  </param>
	  <param>
	      <key>--bcp_ext</key>
	      <value>out</value>
	  </param>
	  <param>
	      <key>--bcpmode</key>
	      <value>in</value>
	  </param>
	  <param>
	      <key>--ignore_empty_bcp</key>
	      <value>1</value>
	  </param>
	  <param>
	      <key>--batchsize</key>
	      <value>$;BCP_BATCH_SIZE$;</value>
	  </param>
	  <param>
	      <key>--infile</key>
	      <value>$;I_BCP_FILE$;</value>
	  </param>
	  <param>
	      <key>--debug_level</key>
	      <value>$;DEBUG$;</value>
	  </param>
	  <param>
	      <key>--logfile</key>
	      <value>$;WORKFLOW_REPOSITORY$;/flatFileToChado.pl.$;I_TABLE_NAME$;.log</value>
	  </param>
	  <param>
	      <key>stdout</key>
	      <value>$;WORKFLOW_REPOSITORY$;/flatFileToChado.pl.$;I_TABLE_NAME$;.stdout</value>
	  </param>
	  <param>
	      <key>stderr</key>
	      <value>$;WORKFLOW_REPOSITORY$;/flatFileToChado.pl.$;I_TABLE_NAME$;.stderr</value>
      </param>
      </command>
      <command>
	      <type>RunUnixCommand</type>
	      <name>compress bcp file</name>
	      <state>incomplete</state>
	      <executable>$;BIN_DIR$;/compress_file</executable>
              <param>
	          <key>--file</key>
	          <value>$;I_BCP_FILE$;</value>
	      </param>
	      <param>
	          <key>--output</key>
	          <value>$;OUTPUT_DIRECTORY$;/$;I_TABLE_NAME$;.out</value>
	      </param>
	      <param>
	          <key>--compress</key>
	          <value>$;GZIP_BCP$;</value>
	      </param>
      </command>
  </commandSet>
</commandSetRoot>
