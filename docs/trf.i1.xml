<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
        <name>trf compute</name>
        <command>
            <type>RunUnixCommand</type>
            <name>trf</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/trf_wrapper</executable>
            <param>  
                <key>--trf_exec</key>
                <value>$;TRF_EXEC$;</value>
            </param>
            <param>  
                <key>--input</key>
                <value>$;I_FILE_PATH$;</value>
            </param>
            <param>  
                <key>--match</key>
                <value>$;MATCH$;</value>
            </param>
            <param>  
                <key>--mismatch</key>
                <value>$;MISMATCH$;</value>
            </param>
            <param>  
                <key>--delta</key>
                <value>$;DELTA$;</value>
            </param>
            <param>  
                <key>--pm</key>
                <value>$;PM$;</value>
            </param>
            <param>  
                <key>--pi</key>
                <value>$;PI$;</value>
            </param>
            <param>  
                <key>--minscore</key>
                <value>$;MINSCORE$;</value>
            </param>
            <param>  
                <key>--maxperiod</key>
                <value>$;MAXPERIOD$;</value>
            </param>
            <param>  
                <key>--other_opts</key>
                <value>'-d $;OTHER_OPTS$;'</value>
            </param>
            <param>  
                <key>--output_dir</key>
                <value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>relocate output</name>
            <state>incomplete</state>
            <executable>find</executable>
            <arg>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$; -type f -regex '.*\/$;I_FILE_BASE$;\..*$;MATCH$;\.$;MISMATCH$;\.$;DELTA$;\.$;PM$;\.$;PI$;\.$;MINSCORE$;\.$;MAXPERIOD$;\.dat' -exec mv \{\} $;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.raw \;</arg>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>trf2bsml</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/trf2bsml</executable>
            <param>  
                <key>--input</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.raw</value>
            </param>
            <param>  
                <key>--output</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml</value>
            </param>
            <param>  
                <key>--project</key>
                <value>$;PROJECT$;</value>
            </param>
            <param>  
                <key>--id_repository</key>
                <value>$;PROJECT_ID_REPOSITORY$;</value>
            </param>
            <param>  
                <key>--gzip_output</key>
                <value>$;COMPRESS_BSML_OUTPUT$;</value>
            </param>
            <param>  
                <key>--fasta_input</key>
                <value>$;I_FILE_PATH$;</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>store config params</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/analysis2bsml</executable>
            <param>  
                <key>--ergatis_control_file</key>
                <value>$;ERGATIS_DIR$;.ini</value>
            </param>
            <param>  
                <key>--conf</key>
                <value>$;COMPONENT_CONFIG$;</value>
            </param>
            <param>  
                <key>--bsml_file</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml</value>
            </param>
            <param>  
                <key>--componenturl</key>
                <value>$;WORKFLOW_REPOSITORY$;/component.xml</value>
            </param>
            <param>  
                <key>--pipelineurl</key>
                <value>$;PIPELINE_XML$;</value>
            </param>
            <param>  
                <key>--analysis_id</key>
                <value>$;COMPONENT_NAME$;_analysis</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>dtd validation</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/dtdValid.pl</executable>
            <param>  
                <key>-d</key>
                <value>$;DOCS_DIR$;/bsml3_1.dtd</value>
            </param>
            <arg>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml</arg>
        </command>
    </commandSet>
</commandSetRoot>
