<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
        <name>fastx_trimming</name>
		<command>
            <type>RunUnixCommand</type>
            <name>fastx_trimming</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/fastx_trimming</executable>            
            <param>
                <key>--infile</key>
                <value>$;I_FILE_PATH$;</value>
            </param>
            <param>
                <key>--outdir</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;</value>
            </param>
            <param>
                <key>--quality</key>
                <value>$;QUALITY_STRING$;</value>
            </param>
            <param>
                <key>--first_base</key>
                <value>'$;FIRST_BASE$;'</value>
            </param>
            <param>
                <key>--last_base</key>
                <value>'$;LAST_BASE$;'</value>
            </param>
            <param>
                <key>--fastx_bin_dir</key>
                <value>$;FASTX_BIN_DIR$;</value>
            </param>
            <param>
                <key>stdout</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/fastx_trimming.stdout</value>
            </param>
            <param>
                <key>stderr</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/fastx_trimming.stderr</value>
            </param>
            <arg>$;OTHER_ARGS$;</arg>
        </command>
    </commandSet>
</commandSetRoot>
