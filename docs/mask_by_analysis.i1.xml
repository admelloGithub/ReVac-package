<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
        <name>mask_by_analysis compute</name>
        <command>
            <type>RunUnixCommand</type>
            <name>mask_by_analysis</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/mask_by_analysis</executable>
            <param>  
                <key>--input</key>
                <value>$;I_FILE_PATH$;</value>
            </param>
            <param>  
                <key>--output</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;</value>
            </param>
            <param>  
                <key>--output_prefix</key>
                <value>$;I_FILE_BASE$;.$;COMPONENT_NAME$;</value>
            </param>
            <param>  
                <key>--analysis_types</key>
                <value>'$;ANALYSIS_TYPES$;'</value>
            </param>
            <param>  
                <key>--feature_types</key>
                <value>'$;FEATURE_CLASSES$;'</value>
            </param>
            <param>  
                <key>--mask_char</key>
                <value>'$;MASK_CHARACTER$;'</value>
            </param>
            <param>  
                <key>--random</key>
                <value>'$;RANDOM$;'</value>
            </param>
            <param>  
                <key>--softmask</key>
                <value>'$;SOFTMASK$;'</value>
            </param>
            <param>  
                <key>--gzip_output</key>
                <value>$;COMPRESS_BSML$;</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>store config params</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/analysis2bsml</executable>
            <param>  
                <key>--ergatis_control_file</key>
                <value>$;ERGATIS_DIR$;.ini</value>
            </param>
            <param>  
                <key>--conf</key>
                <value>$;COMPONENT_CONFIG$;</value>
            </param>
            <param>  
                <key>--bsml_file</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml</value>
            </param>
            <param>  
                <key>--componenturl</key>
                <value>$;WORKFLOW_REPOSITORY$;/component.xml</value>
            </param>
            <param>  
                <key>--pipelineurl</key>
                <value>$;PIPELINE_XML$;</value>
            </param>
            <param>  
                <key>--analysis_id</key>
                <value>$;COMPONENT_NAME$;_analysis</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>dtd validation</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/dtdValid.pl</executable>
            <param>  
                <key>-d</key>
                <value>$;DOCS_DIR$;/bsml3_1.dtd</value>
            </param>
            <arg>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml</arg>
        </command>
    </commandSet>
</commandSetRoot>
