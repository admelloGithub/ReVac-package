<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"			xsi:schemaLocation='commandSet.xsd'>
  <commandSet type="serial">
    <name>check_small_genome_db</name>
      <command>
            <type>RunUnixCommand</type>
	  <state>incomplete</state>
	  <name>Create database output directories</name>
	  <executable>mkdir</executable>
 	  <arg>-p -m 777 $;WORKFLOW_REPOSITORY$;/$;DATABASE$;/load $;WORKFLOW_REPOSITORY$;/$;DATABASE$;/gip $;WORKFLOW_REPOSITORY$;/$;DATABASE$;/sgc</arg>
      </command>
      <commandSet type="parallel">
      <command>
	  <type>RunUnixCommand</type>
	  <name>check_small_genome_db load</name>
	  <state>incomplete</state>
	  <executable>$;BIN_DIR$;/check_small_genome_db</executable>
	  <param>
	      <key>--workflow_enabled</key>
	      <value>1</value>
	  </param>
	  <param>
	      <key>--username</key>
	      <value>$;USERNAME$;</value>
	  </param>
	  <param>
	      <key>--password</key>
	      <value>$;PASSWORD$;</value>
	  </param>
	  <param>
	      <key>--database</key>
	      <value>$;DATABASE$;</value>
	  </param>
	  <param>
	      <key>--stage</key>
	      <value>load</value>
	  </param>
	  <param>
	      <key>--errorlog</key>
	      <value>$;WORKFLOW_REPOSITORY$;/$;DATABASE$;/load/check_small_genome_db.dbi.errorlog.log</value>
	  </param>
	  <param>
	      <key>--logfile</key>
	      <value>$;WORKFLOW_REPOSITORY$;/$;DATABASE$;/load/check_small_genome_db.dbi.logfile.log</value>
	  </param>
	  <param>
	      <key>stdout</key>
	      <value>$;WORKFLOW_REPOSITORY$;/$;DATABASE$;/load/check_small_genome_db.dbi.stdout</value>
	  </param>
	  <param>
	      <key>stderr</key>
	      <value>$;WORKFLOW_REPOSITORY$;/$;DATABASE$;/load/check_small_genome_db.dbi.stderr</value>
	  </param>
 	  <arg>--tigr_ann_only</arg>
      </command>
      <command>
	  <type>RunUnixCommand</type>
	  <name>check_small_genome_db gip</name>
	  <state>incomplete</state>
	  <executable>$;BIN_DIR$;/check_small_genome_db</executable>
	  <param>
	      <key>--workflow_enabled</key>
	      <value>1</value>
	  </param>
	  <param>
	      <key>--username</key>
	      <value>$;USERNAME$;</value>
	  </param>
	  <param>
	      <key>--password</key>
	      <value>$;PASSWORD$;</value>
	  </param>
	  <param>
	      <key>--database</key>
	      <value>$;DATABASE$;</value>
	  </param>
	  <param>
	      <key>--stage</key>
	      <value>gip</value>
	  </param>
	  <param>
	      <key>--server</key>
	      <value>$;SERVER$;</value>
	  </param>
	  <param>
	      <key>--errorlog</key>
	      <value>$;WORKFLOW_REPOSITORY$;/$;DATABASE$;/gip/check_small_genome_db.dbi.errorlog.log</value>
	  </param>
	  <param>
	      <key>--logfile</key>
	      <value>$;WORKFLOW_REPOSITORY$;/$;DATABASE$;/gip/check_small_genome_db.dbi.logfile.log</value>
