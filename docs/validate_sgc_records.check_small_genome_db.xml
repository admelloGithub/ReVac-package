	      <value>$;WORKFLOW_REPOSITORY$;/$;DATABASE$;/sgc/check_small_genome_db.dbi.logfile.log</value>
	  </param>
	  <param>
	      <key>stdout</key>
	      <value>$;WORKFLOW_REPOSITORY$;/$;DATABASE$;/sgc/check_small_genome_db.dbi.stdout</value>
	  </param>
	  <param>
	      <key>stderr</key>
	      <value>$;WORKFLOW_REPOSITORY$;/$;DATABASE$;/sgc/check_small_genome_db.dbi.stderr</value>
	  </param>
 	  <arg>--tigr_ann_only</arg>
