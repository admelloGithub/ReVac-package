<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
        <state>incomplete</state>
        <name>cuffdiff</name>
        <!--Processing-->
        <command>
            <type>RunUnixCommand</type>
            <name>create output directory</name>
            <state>incomplete</state>
            <executable>mkdir</executable>
            <arg>-p -m 777 $;OUTPUT_DIRECTORY$;</arg>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>create temp directory</name>
            <state>incomplete</state>
            <executable>mkdir</executable>
            <arg>-p -m 777 $;TMP_DIR$;</arg>
        </command>
        <!--Iterator-->
        <INCLUDE file="$;DOCS_DIR$;/paired_list_iterator_template.xml" keys="$;ITERATOR_NAME$;=ITERATOR1,$;ITERATOR_XML$;=ITERATOR1_XML">
		<!--Postprocessing-->
        <command>
            <type>RunUnixCommand</type>
            <name>create differentially expressed CDS list</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/generate_list_file</executable>
            <param>  
                <key>--directory</key>
                <value>$;OUTPUT_DIRECTORY$;</value>
            </param>
            <param>  
                <key>--regex</key>
                <value>".*cds.*\.diff"</value>
            </param>
            <param>  
                <key>--output_list</key>
                <value>$;OUTPUT_CDS_DIFF$;</value>
            </param>
            <param>
				<key>stdout</key>
				<value>$;TMP_DIR$;/generate_list_file.cds.stdout</value>
			</param>
			<param>
				<key>stderr</key>
				<value>$;TMP_DIR$;/generate_list_file.cds.stderr</value>
			</param>
			<arg>--v</arg>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>create differentially expressed gene list</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/generate_list_file</executable>
