<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
	<name>Execute SQL</name>
	<command>
	    <type>RunUnixCommand</type>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/sql2DB</executable>
	    <name>Send SQL to chado</name>
	    <param>
		<key>--username</key>
		<value>chado_admin</value>
	    </param>
	    <param>
		<key>--password</key>
		<value>chado_admin99</value>
	    </param>
	    <param>
		<key>--database</key>
		<value>$;DATABASE$;</value>
	    </param>
	    <param>
		<key>--database_type</key>
		<value>$;DATABASE_TYPE$;</value>
	    </param>
	    <param>
		<key>--server</key>
		<value>$;SERVER$;</value>
	    </param>
	    <param>
		<key>--log</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.log</value>
	    </param>
	    <param>
		<key>--file</key>
		<value>$;I_FILE_PATH$;</value>
	    </param>
	    <param>
		<key>--force</key>
		<value>1</value>
	    </param>
	    <param>
		<key>--exec</key>
		<value>1</value>
	    </param>
	    <param>
		<key>stdout</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.stdout</value>
	    </param>
	    <param>
		<key>stderr</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.stderr</value>
	    </param>
        </command>
      <command>
	      <type>RunUnixCommand</type>
	      <name>compress delete file</name>
	      <state>incomplete</state>
	      <executable>$;BIN_DIR$;/compress_file</executable>
              <param>
	          <key>--file</key>
	          <value>$;I_FILE_PATH$;</value>
	      </param>
	      <param>
	          <key>--output</key>
	          <value>$;OUTPUT_DIRECTORY$;/$;I_FILE_BASE$;.$;I_FILE_EXT$;</value>
	      </param>
	      <param>
	          <key>--compress</key>
	          <value>$;GZIP_SQL$;</value>
	      </param>
      </command>
    </commandSet>
</commandSetRoot>
