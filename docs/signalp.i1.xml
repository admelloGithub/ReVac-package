<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
        <name>signalp compute</name>
        <command>
            <type>RunUnixCommand</type>
            <name>Create scratch dir for signalp output</name>
            <state>incomplete</state>
            <executable>mkdir</executable>
            <arg>-p -m 777 $;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;</arg>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>Run signalp</name>
            <state>incomplete</state>
            <executable>$;SIGNALP_EXEC$;</executable>
            <arg>-t $;ORGANISM_TYPE$; -s '$;PREDICTION_METHOD$;' -c $;TRUNCATED_LENGTH$; -T $;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$; -f summary $;ARGS$; $;I_FILE_PATH$;</arg>
            <param>
                <key>stdout</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.raw</value>
            </param>
			<param>
                <key>stderr</key>
                <value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.stderr</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>Rename signalp graphics files</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/signalp_graphics_rename</executable>
            <param>
                <key>--input_path</key>
                <value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;</value>
            </param>
            <param>
                <key>--output_path</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/</value>
            </param>
            <param>
                <key>--output_prefix</key>
                <value>$;I_FILE_BASE$;.$;COMPONENT_NAME$;</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>Convert signalp raw output to BSML</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/signalp2bsml</executable>
            <param>
                <key>--input</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.raw</value>
            </param>
            <param>
                <key>--query_file_path</key>
                <value>$;I_FILE_PATH$;</value>
            </param>
            <param>
                <key>--output</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml</value>
            </param>
            <param>
                <key>--project</key>
                <value>$;PROJECT$;</value>
            </param>
            <param>
                <key>--id_repository</key>
                <value>$;PROJECT_ID_REPOSITORY$;</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>store config params for SignalP Best</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/analysis2bsml</executable>
            <param>
                <key>--ergatis_control_file</key>
                <value>$;ERGATIS_DIR$;.ini</value>
            </param>
            <param>
                <key>--conf</key>
                <value>$;COMPONENT_CONFIG$;</value>
            </param>
            <param>
                <key>--bsml_file</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml</value>
            </param>
            <param>
                <key>--componenturl</key>
                <value>$;WORKFLOW_REPOSITORY$;/component.xml</value>
            </param>
            <param>
                <key>--pipelineurl</key>
                <value>$;PIPELINE_XML$;</value>
            </param>
            <param>
                <key>--analysis_id</key>
                <value>$;COMPONENT_NAME$;_best_analysis</value>
            </param>
        </command>
        <command>
