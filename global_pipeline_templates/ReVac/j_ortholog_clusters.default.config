[interface]
classification = clustering

[parameters]
$;PVALCUT$;=1e-5
$;PCOVERAGE_CUTOFF$;=70

$;PIDENTITY_CUTOFF$;=
$;PSIMILARITY_CUTOFF$;=

$;MAX_MULTI_ALIGNMENT$;=100
;; Jaccard coefficient cutoff for pruning edges from generated clusters - should be a value between 0 and 1, inclusive
$;JACCARD_CUTOFF$;=0.0
;; set this to 1 if the Seq-data-import.identifiers in the input BSML are not guaranteed to match the 
;; ids of the corresponding linked features
$;USE_FEATURE_IDS_IN_FASTA$;=0

[input]
$;QUERY_BSML_FILE_LIST$;=$;REPOSITORY_ROOT$;/output_repository/genbank2bsml/$;PIPELINEID$;_default/genbank2bsml.bsml.list

$;MATCH_BSML_FILE_LIST$;=$;REPOSITORY_ROOT$;/output_repository/wu-blastp/$;PIPELINEID$;_default/wu-blastp.bsml.list

;; List of BSML output files generated by clustalw component for a jaccard run
$;JACCARD_OUTPUT_LIST$;=$;REPOSITORY_ROOT$;/output_repository/muscle/$;PIPELINEID$;_jaccard/muscle.bsml.list

[output]
$;OUTPUT_TOKEN$;=default
$;OUTPUT_DIRECTORY$;=$;REPOSITORY_ROOT$;/output_repository/$;COMPONENT_NAME$;/$;PIPELINEID$;_$;OUTPUT_TOKEN$;
$;FASTA_OUTPUT_LIST$;=$;OUTPUT_DIRECTORY$;/$;COMPONENT_NAME$;.fsa.list

[component]
$;COMPONENT_NAME$;=j_ortholog_clusters
$;DESCRIPTION$;=none
$;WORKFLOW_REPOSITORY$;=$;REPOSITORY_ROOT$;/workflow/runtime/$;COMPONENT_NAME$;/$;PIPELINEID$;_$;OUTPUT_TOKEN$;
$;PIPELINE_TOKEN$;=unnamed

;The version,revision,tag here is set by an interpolated CVS tag
$;VERSION$;=2.0
$;RELEASE_TAG$;=$Name$
$;REVISION$;=$Revision$

$;TEMPLATE_XML$;=$;DOCS_DIR$;/$;COMPONENT_NAME$;.xml

;no iterators in this component
;$;ITERATOR1$; = i1
;$;ITERATOR1_XML$; = $;DOCS_DIR$;/$;COMPONENT_NAME$;.$;ITERATOR1$;.xml

;Distributed options
$;GROUP_COUNT$;=150
$;NODISTRIB$;=0

;the following keys are replaced at runtime by the invocation script
$;COMPONENT_CONFIG$;=
$;COMPONENT_XML$;=
$;PIPELINE_XML$;=
$;PIPELINEID$;=

[include]
$;PROJECT_CONFIG$;=/usr/local/scratch/admello/workflow/project.config


;; This file contains any component configuration options that are common across 
;; all Ergatis components. Any components added here should be displayed in the interface
;; when adding any component to a pipeline

[dce]
;; These configuration options only apply to any distributed components 
$;GROUP$;=$;PROJECT_CODE$;
$;OS$;=linux
$;MEMORY$;=
$;HOSTS$;=
$;QUEUE$;=
$;DURATION$;=
$;PRIORITY$;=
$;PASSTHROUGH$;=
