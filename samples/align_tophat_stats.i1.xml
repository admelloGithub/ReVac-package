            </param>
	    <param>
                <key>--args</key>
                <value>$;OTHER_PARAMETERS$;</value>
            </param>
            <param>
                <key>stdout</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/align_tophat_stats.stdout</value>
            </param>
            <param>
                <key>stderr</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/align_tophat_stats.stderr</value>
            </param>
        </command>
    </commandSet>
</commandSetRoot>
