<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
        <name>auto annotate</name>
        <command>
            <type>RunUnixCommand</type>
            <name>auto annotate compute</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/auto_annotate</executable>
            <param>
               <key>--input_file</key>
               <value>$;I_FILE_PATH$;</value>
            </param>
            <param>
               <key>--hmm_analysis</key>
               <value>$;HMM_ANALYSIS$;</value>
            </param>
            <param>
               <key>--ber_analysis</key>
               <value>$;BER_ANALYSIS$;</value>
            </param>
            <param>
               <key>--hmm_info_db</key>
               <value>$;HMM_INFO_DB$;</value>
            </param>
            <param>
               <key>--panda_header_offsets</key>
               <value>$;PANDA_HEADER_OFFSETS$;</value>
            </param>
            <param>
               <key>--length_cutoff</key>
               <value>$;LENGTH_CUTOFF$;</value>
            </param>
            <param>
               <key>--panda_header_file</key>
               <value>$;PANDA_HEADER_FILE$;</value>
            </param>
            <param>
               <key>--role_info_db</key>
               <value>$;ROLE_INFO_DB$;</value>
            </param>
            <param>
               <key>--gzip_output</key>
               <value>$;COMPRESS_BSML_OUTPUT$;</value>
            </param>
            <param>
               <key>--output_bsml</key>
               <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml</value>
            </param>
            <param>
               <key>stderr</key>
               <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.stderr</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>store config params</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/analysis2bsml</executable>
            <param>  
                <key>--ergatis_control_file</key>
                <value>$;ERGATIS_DIR$;.ini</value>
            </param>
            <param>  
                <key>--conf</key>
                <value>$;COMPONENT_CONFIG$;</value>
            </param>
            <param>  
                <key>--bsml_file</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml</value>
            </param>
            <param>  
                <key>--componenturl</key>
                <value>$;WORKFLOW_REPOSITORY$;/component.xml</value>
            </param>
            <param>  
                <key>--pipelineurl</key>
                <value>$;PIPELINE_XML$;</value>
            </param>
            <param>  
                <key>--analysis_id</key>
                <value>$;COMPONENT_NAME$;_analysis</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>dtd validation</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/dtdValid.pl</executable>
            <param>  
                <key>-d</key>
                <value>$;DOCS_DIR$;/bsml3_1.dtd</value>
            </param>
            <arg>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml</arg>
        </command>
    </commandSet>
</commandSetRoot>
