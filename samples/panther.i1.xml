<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
        <name>panther compute</name>
        <command>
            <type>RunUnixCommand</type>
            <name>panther</name>
            <state>incomplete</state>
            <executable>$;PANTHER_EXEC$;</executable>
            <arg>-n</arg>
            <param>  
                <key>-D</key>
                <value>$;DISPLAY_TYPE$;</value>
            </param>
            <param>  
                <key>-E</key>
                <value>$;HMM_EVALUE_CUTOFF$;</value>
            </param>
            <param>  
                <key>-B</key>
                <value>$;BLASTALL_EXEC$;</value>
            </param>
            <param>  
                <key>-H</key>
                <value>$;HMMSEARCH_EXEC$;</value>
            </param>
            <param>  
                <key>-z</key>
                <value>$;GUNZIP_EXEC$;</value>
            </param>
            <param>  
                <key>-T</key>
                <value>$;TMP_DIR$;</value>
            </param>
            <param>  
                <key>-l</key>
                <value>$;PANTHER_HMM_LIBRARY$;</value>
            </param>
            <param>  
                <key>-i</key>
                <value>$;I_FILE_PATH$;</value>
            </param>
            <param>  
                <key>-c</key>
                <value>1</value>
            </param>
            <param>  
                <key>stdout</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.raw</value>
            </param>
            <param>  
                <key>stderr</key>
                <value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.stderr</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>panther2bsml</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/panther2bsml</executable>
            <param>  
                <key>--input</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.raw</value>
            </param>
            <param>  
                <key>--output</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml</value>
            </param>
            <param>  
                <key>--query_file_path</key>
                <value>$;I_FILE_PATH$;</value>
            </param>
            <param>  
                <key>--gzip_output</key>
                <value>$;COMPRESS_BSML_OUTPUT$;</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>store config params</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/analysis2bsml</executable>
            <param>  
                <key>--ergatis_control_file</key>
                <value>$;ERGATIS_DIR$;.ini</value>
            </param>
            <param>  
                <key>--conf</key>
                <value>$;COMPONENT_CONFIG$;</value>
            </param>
            <param>  
                <key>--bsml_file</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml</value>
            </param>
            <param>  
                <key>--componenturl</key>
                <value>$;WORKFLOW_REPOSITORY$;/component.xml</value>
            </param>
            <param>  
                <key>--pipelineurl</key>
                <value>$;PIPELINE_XML$;</value>
            </param>
            <param>  
                <key>--analysis_id</key>
                <value>$;COMPONENT_NAME$;_analysis</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>dtd validation</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/dtdValid.pl</executable>
            <param>  
                <key>-d</key>
                <value>$;DOCS_DIR$;/bsml3_1.dtd</value>
            </param>
            <arg>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml</arg>
        </command>
    </commandSet>
</commandSetRoot>
