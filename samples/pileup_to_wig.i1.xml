<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">

        <command>
            <type>RunUnixCommand</type>
            <name>pileup2wig script</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/pileup2wig</executable>            
            <param>
            <key>stdout</key>
            <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.pileup2wig.stdout</value>
            </param>
            <param>
            <key>stderr</key>
            <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.pileup2wig.stderr</value>
            </param>
            <arg>--v --p $;I_FILE_PATH$; --o $;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;</arg>
        </command>
    
    </commandSet>
</commandSetRoot>
