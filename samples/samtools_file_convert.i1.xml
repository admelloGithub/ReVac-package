<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
        <name>samtools_file_convert</name>
		<command>
            <type>RunUnixCommand</type>
            <name>samtools_file_convert</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/samtools_file_convert</executable>
            <param>
                <key>--infile</key>
                <value>$;I_FILE_PATH$;</value>
            </param>
            <param>
                <key>--infile_format</key>
                <value>$;INPUT_FILE_FORMAT$;</value>
            </param>
            <param>
                <key>--options</key>
                <value>$;OPTIONS$;</value>
            </param>
            <param>
                <key>--reffile</key>
                <value>$;REFERENCE_FASTA$;</value>
            </param>
            <param>
                <key>--outdir</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;</value>
            </param>
            <param>
                <key>--samtools_bin_dir</key>
                <value>$;SAMTOOLS_BIN_DIR$;</value>
            </param>
            <param>
                <key>--samtools_view_options</key>
                <value>$;SAMTOOLS_VIEW_PARAMETERS$;</value>
            </param>
            <param>
                <key>--samtools_sort_options</key>
                <value>$;SAMTOOLS_SORT_PARAMETERS$;</value>
            </param>
            <param>
                <key>stdout</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/samtools_file_convert.stdout</value>
            </param>
            <param>
                <key>stderr</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/samtools_file_convert.stderr</value>
            </param>
            <arg>$;OTHER_ARGS$;</arg>
        </command>
    </commandSet>
</commandSetRoot>
