<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
        <name>clustalw compute</name>
        <command>
            <type>RunUnixCommand</type>
            <name>relocate query file</name>
            <state>incomplete</state>
            <executable>cp</executable>
            <arg>$;I_FILE_PATH$; $;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/</arg>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>clustalw</name>
            <state>incomplete</state>
            <executable>$;CLUSTALW_EXEC$;</executable>
            <param>  
                <key>-output</key>
                <value>$;OUTPUT_FORMAT$;</value>
            </param>
            <param>  
                <key>-type</key>
                <value>$;TYPE$;</value>
            </param>
            <param>  
                <key>-infile</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_NAME$;</value>
            </param>
            <param>  
                <key>-outfile</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.clw</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>validate clustalw output</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/validate_clustalw_output</executable>
            <param>  
                <key>-fastafile</key>
                <value>$;I_FILE_PATH$;</value>
            </param>
            <param>  
                <key>-clwfile</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.clw</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>msf2bsml</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/MSF2Bsml</executable>
            <param>  
                <key>--msffile</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.clw</value>
            </param>
            <param>  
                <key>--output</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml</value>
            </param>
            <param>  
                <key>--analysis_conf</key>
                <value>'$;ANALYSIS_CONF$;'</value>
            </param>
            <param>  
                <key>--dnd_file</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.dnd</value>
            </param>
            <param>  
                <key>--fastafile</key>
                <value>$;I_FILE_PATH$;</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>store config params</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/analysis2bsml</executable>
            <param>  
                <key>--ergatis_control_file</key>
                <value>$;ERGATIS_DIR$;.ini</value>
            </param>
            <param>  
                <key>--conf</key>
                <value>$;ANALYSIS_CONF$;</value>
            </param>
            <param>  
                <key>--bsml_file</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml</value>
            </param>
            <param>  
                <key>--componenturl</key>
                <value>$;WORKFLOW_REPOSITORY$;/component.xml</value>
            </param>
            <param>  
                <key>--pipelineurl</key>
                <value>$;PIPELINE_XML$;</value>
            </param>
            <param>  
                <key>--analysis_id</key>
                <value>first</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>compress raw output</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/compress_file</executable>
            <param>  
                <key>--file</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.clw</value>
            </param>
            <param>  
                <key>--compress</key>
                <value>$;COMPRESS_RAW_OUTPUT$;</value>
            </param>
            <param>  
                <key>--list_file</key>
                <value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.clw.list</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>dtd validation</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/dtdValid.pl</executable>
            <param>  
                <key>-d</key>
                <value>$;DOCS_DIR$;/bsml3_1.dtd</value>
            </param>
            <arg>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml</arg>
        </command>
    </commandSet>
</commandSetRoot>
