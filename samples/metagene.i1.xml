            </param>
            <param>  
                <key>--output_directory</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/</value>
            </param>
            <param>  
                <key>--project</key>
                <value>$;PROJECT$;</value>
            </param>
            <param>  
                <key>--id_repository</key>
                <value>$;PROJECT_ID_REPOSITORY$;</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>get_seq_by_metagene</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/get_seq_by_metagene</executable>
            <param>  
                <key>--fasta_input</key>
                <value>$;I_FILE_PATH$;</value>
            </param>
            <param>  
                <key>--input_file</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.raw</value>
