                <key>--output</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.gbk</value>
            </param>
            <param>  
                <key>--format</key>
                <value>$;OUT_FORMAT$;</value>
            </param>
            <param>  
                <key>--prefix</key>
                <value>$;GENE_PREFIX$;</value>
            </param>
        </command>
