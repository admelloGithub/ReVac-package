<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"			xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
	<name>fastaToBsmlWorkingGeneModels</name>
	<command>
	    <type>RunUnixCommand</type>
	    <state>incomplete</state>
	    <name>Create the FASTA output directory</name>
	    <executable>mkdir</executable>
	    <arg>-p -m 777 $;FASTA_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/</arg>
	</command>
	<command>
	    <type>RunUnixCommand</type>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/fastaToBsmlWorkingGeneModels</executable>
	    <name>fastaToBsmlWorkingGeneModels</name>
	    <param>
		<key>--database</key>
		<value>$;DATABASE$;</value>
	    </param>
	    <param>
		<key>--sequenceId</key>
		<value>$;SEQUENCE_ID$;</value>
	    </param>
	    <param>
		<key>--infile</key>
		<value>$;INPUT_FASTA_FILE$;</value>
	    </param>
	    <param>
		<key>--outfile</key>
		<value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;SEQUENCE_ID$;.bsml</value>
	    </param>
	    <param>
		<key>--logfile</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;SEQUENCE_ID$;.fastaToBsmlWorkingGeneModels.pl.log</value>
	    </param>
	    <param>
		<key>--debug_level</key>
		<value>$;DEBUG$;</value>
	    </param>
	    <param>
		<key>--genus</key>
		<value>$;GENUS$;</value>
	    </param>
	    <param>
		<key>--species</key>
		<value>'$;SPECIES$;'</value>
	    </param>
	    <param>
		<key>--id-repository</key>
		<value>$;PROJECT_ID_REPOSITORY$;</value>
	    </param>
	    <param>
		<key>stdout</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/fastaToBsmlWorkingGeneModels.$;SEQUENCE_ID$;.stdout</value>
	    </param>
	    <param>
		<key>stderr</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/fastaToBsmlWorkingGeneModels.$;SEQUENCE_ID$;.stderr</value>
	    </param>
	</command>
	<command>
	    <type>RunUnixCommand</type>
	    <state>incomplete</state>
	    <name>DTD validation</name>
	    <executable>$;BIN_DIR$;/dtdValid.pl</executable>
	    <arg>-d $;BSML_DTD$; $;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;SEQUENCE_ID$;.bsml</arg>
	</command>
	<command>
	    <type>RunUnixCommand</type>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/profileBSMLFile</executable>
	    <name>profileBSMLFile</name>
	    <param>
		<key>--infile</key>
		<value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;SEQUENCE_ID$;.bsml</value>
	    </param>
	    <param>
		<key>--database</key>
		<value>$;DATABASE$;</value>
	    </param>
	    <param>
		<key>--asmbl_id</key>
		<value>$;SEQUENCE_ID$;</value>
	    </param>
	    <param>
		<key>--logfile</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;SEQUENCE_ID$;.profileBSMLFile.pl.log</value>
	    </param>
	    <param>
		<key>stdout</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/profileBSMLFile.$;SEQUENCE_ID$;.stdout</value>
	    </param>
	    <param>
		<key>stderr</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/profileBSMLFile.$;SEQUENCE_ID$;.stderr</value>
	    </param>
	</command>
    </commandSet>
</commandSetRoot>
