<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
	<name>Process all OBO filesr</name>
	<command>
	    <type>RunUnixCommand</type>
	    <state>incomplete</state>
	    <name>Create temporary cvtermpath_closure output directory</name>
	    <executable>mkdir</executable>
	    <arg>-p -m 777 $;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;CV_ID$;</arg>
        </command>
	<command>
	    <type>RunUnixCommand</type>
	    <state>incomplete</state>
	    <name>Generate cvtermpath records</name>
	    <executable>$;BIN_DIR$;/cvtermpath_closure</executable>
	    <param>
		<key>--username</key>
		<value>$;USERNAME$;</value>
	    </param>
	    <param>
		<key>--password</key>
		<value>$;PASSWORD$;</value>
	    </param>
	    <param>
		<key>--database</key>
		<value>$;DATABASE$;</value>
	    </param>
	    <param>	  
		<key>--database_type</key>
		<value>$;DATABASE_TYPE$;</value>
	    </param>
	    <param>
		<key>--server</key>
		<value>$;SERVER$;</value>
	    </param>
	    <param>
		<key>--debug_level</key>
		<value>$;DEBUG$;</value>
	    </param>
	    <param>
		<key>--cv_id</key>
		<value>$;CV_ID$;</value>
	    </param>
	    <param>
		<key>--logfile</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;CV_ID$;/cvtermpath_closure.pl.log</value>
	    </param>
	    <param>
		<key>--outdir</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;CV_ID$;</value>
	    </param>
	    <param>
		<key>--cache_dir</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;CV_ID$;</value>
	    </param>
	    <param>
		<key>stdout</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;CV_ID$;/cvtermpath_closure.pl.stdout</value>
	    </param>
	    <param>
		<key>stderr</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;CV_ID$;/cvtermpath_closure.pl.stderr</value>
	    </param>
        </command>
	<command>
	    <type>RunUnixCommand</type>
	    <state>incomplete</state>
	    <name>Load the cvtermpath BCP file contents</name>
	    <executable>$;BIN_DIR$;/flatFileToChado</executable>
	    <param>
		<key>--username</key>
		<value>$;USERNAME$;</value>
	    </param>
	    <param>
		<key>--password</key>
		<value>$;PASSWORD$;</value>
	    </param>
	    <param>
		<key>--database</key>
		<value>$;DATABASE$;</value>
	    </param>
	    <param>	  
		<key>--database_type</key>
		<value>$;DATABASE_TYPE$;</value>
	    </param>
	    <param>
		<key>--bcp_ext</key>
		<value>out</value>
	    </param>
	    <param>
		<key>--server</key>
		<value>$;SERVER$;</value>
	    </param>
	    <param>
		<key>--debug_level</key>
		<value>5</value>
	    </param>
	    <param>
		<key>--bcpmode</key>
		<value>in</value>
	    </param>
	    <param>
		<key>--logfile</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;CV_ID$;/flatFileToChado.pl.log</value>
	    </param>
	    <param>
		<key>--directory</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;CV_ID$;</value>
	    </param>
	    <param>
		<key>stdout</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;CV_ID$;/flatFileToChado.pl.stdout</value>
	    </param>
	    <param>
		<key>stderr</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;CV_ID$;/flatFileToChado.pl.stderr</value>
	    </param>
        </command>
    </commandSet>
</commandSetRoot>
