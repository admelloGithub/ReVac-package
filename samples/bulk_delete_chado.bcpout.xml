<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"			xsi:schemaLocation='commandSet.xsd'>
  <commandSet type="serial">
    <name>Dump tables' contents to files</name>
      <command>
	  <type>RunUnixCommand</type>
	  <name>dump table</name>
	  <state>incomplete</state>
	  <executable>$;BIN_DIR$;/flatFileToChado</executable>
	  <param>
	      <key>--username</key>
	      <value>chado_admin</value>
	  </param>
	  <param>
	      <key>--password</key>
	      <value>chado_admin99</value>
	  </param>
	  <param>
	      <key>--database</key>
	      <value>$;DATABASE$;</value>
	  </param>
	  <param>
	      <key>--server</key>
	      <value>$;SERVER$;</value>
	  </param>
	  <param>	  
	      <key>--database_type</key>
	      <value>$;DATABASE_TYPE$;</value>
	  </param>
	  <param>
	      <key>--bcp_ext</key>
	      <value>bcp</value>
	  </param>
	  <param>
	      <key>--bcpmode</key>
	      <value>out</value>
	  </param>
	  <param>
	      <key>--batchsize</key>
	      <value>$;BCP_BATCH_SIZE$;</value>
	  </param>
	  <param>
	      <key>--directory</key>
	      <value>$;TMP_DIR$;</value>
	  </param>
	  <param>
	      <key>--table</key>
	      <value>v_$;TABLE$;</value>
	  </param>
	  <param>
	      <key>--debug_level</key>
	      <value>$;DEBUG$;</value>
	  </param>
	  <param>
	      <key>--logfile</key>
	      <value>$;WORKFLOW_REPOSITORY$;/flatFileToChado.pl.out.$;TABLE$;.log</value>
	  </param>
	  <param>
	      <key>stdout</key>
	      <value>$;WORKFLOW_REPOSITORY$;/flatFileToChado.pl.out.$;TABLE$;.stdout</value>
	  </param>
	  <param>
	      <key>stderr</key>
	      <value>$;WORKFLOW_REPOSITORY$;/flatFileToChado.pl.out.$;TABLE$;.stderr</value>
      </param>
      </command>
  </commandSet>
</commandSetRoot>
